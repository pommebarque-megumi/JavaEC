package day1;

public class Wizard {
	private String name;
	private int hp;
	private int mp;
	private Wand wand;

	public String getName() {
		return this.name;
	}
	public int getHp() {
		return this.hp;
	}
	public int getMp() {
		return this.mp;
	}
	public void setName() {
		this.name=name;
	}
}
